<%= form_for(@job) do |f| %>
	<p>
		<%= f.label :title %>
		<%= f.text_field :title %> 
	</p>
	<p>
		<%= f.label :description %>
		<%= f.text_area :description%> 
	</p>
	<p>
		<%= f.label :company %>
		<%= f.text_field :company %> 
	</p>
	<p>
		<%= f.label :link %>
		<%= f.text_field :url %> 
	</p>
	<p><%= f.submit %></p>
<% end %>