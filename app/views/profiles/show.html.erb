<h3>Current Student's profile</h3>
<p>student_id: <%= current_student.id %></p>
<p>profile_id: <%= @profile.id %></p>
<%= @profile.name %>
<%= @profile.description %>
<%= @profile.location %>
<%= @profile.course %>

<%= form_for([@profile, @skill]) do |f| %>
	<p>Skill: <%= f.text_field :name %> </p>
	<p><%= f.submit %></p>
<% end %>
