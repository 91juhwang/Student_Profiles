<h3>All Profiles</h3>

<% @profiles.each do |p| %>
	<li>Student_id: <%= p.student_id %></li>
	<li>Profile id: <%= p.id %></li>
	<li><%= p.name %></li>
	<li><%= p.description %></li>
	<li><%= p.location %></li>
	<li> 
		Skills: <% p.skills.each do |s| %>
			<%= s.name %> 
		<%end %>
	</li>
	<hr>
<% end %>

