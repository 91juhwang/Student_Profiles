<div>
	<h2>
		Developers
	</h2>
	<div class="paginate">
		<% @developers.each do |dev| %>
			<% if dev.profile != nil && dev.profile.experiences.last != nil && dev.profile.educations.last != nil %>
				<h4><%= link_to dev.profile.name, profile_path(dev.profile.id) %></h4>
				<h5>Expeirence:</h5>
				<p>
					Previously a 
					<%= dev.profile.experiences.last.title %> at 
					<%= dev.profile.experiences.last.company %>
				</p>
				<h5>Education: </h5>
				<p>
					Degree in 
					<%= dev.profile.educations.last.major %>
					at
					<%= dev.profile.educations.last.school %>
				</p>
				<h5> Skills: </h5>
				<% dev.profile.skills.each do |s| %>
					<%= s.name %>, 
				<% end %>
			<% end %>
		<% end %>
	</div>
	<div class="paginate_nav">
		<%= will_paginate @developers %>
	</div>	
</div>